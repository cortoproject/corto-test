#package ::corto::test

procedure Case: base=method, kind=procedureKind::METHOD

void fail(string err)
bool assert(bool condition, $condition, $__line)
bool assertEqual(any a, any b, $a, $b, $__line)

struct Result::
    member success: bool, readonly
    member errmsg: string

class SuiteData ::
    member test: test::Case
    member assertCount: uint32
    member result: test::Result, readonly
    int16 construct()
    void setup() virtual
    void teardown() virtual

class Suite{class, defaultType=member, defaultProcedureType=Case}::
    int16 construct()

class Runner::
    member name: string
    member lib: string
    member testcase: string
    member successes: list{Case}, readonly
    member failures: list{Case}, readonly
    member testsRun: int32, readonly
    int16 construct()
    void destruct()
    void runTest() observer
